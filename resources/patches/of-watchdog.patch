+	JMP	syscallSyscall(SB)
+
+TEXT Syscall6(SB),NOSPLIT,$0-80
+	JMP	syscallSyscall6(SB)
+
+TEXT SyscallNoError(SB),NOSPLIT,$0-48
+	JAL	runtimeentersyscall(SB)
+	MOVV	a1+8(FP), R4
+	MOVV	a2+16(FP), R5
+	MOVV	a3+24(FP), R6
+	MOVV	R0, R7
